{
    "name": "parvez/laravel-gallery-manager",
    "description": "A comprehensive Laravel package for managing media files (images, videos, documents, audio) with hierarchical folder structure, supporting both local and AWS S3 storage",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "media", "gallery", "file-manager", "upload", "storage", "s3"],
    "authors": [
        {
            "name": "Parvez Rahman",
            "email": "parvezrohmanfci@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "intervention/image": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Parvez\\GalleryManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parvez\\GalleryManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parvez\\GalleryManager\\GalleryManagerServiceProvider"
            ],
            "aliases": {
                "Gallery": "Parvez\\GalleryManager\\Facades\\Gallery"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
